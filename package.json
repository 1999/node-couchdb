{
  "name": "node-couchdb",
  "version": "0.5.0",
  "homepage": "https://github.com/1999/node-couchdb",
  "author": "Dmitry Sorin <info@staypositive.ru>",
  "description": "node.js package to interact with CouchDB using ETags and your favorite cache layer (memcached, fs, memory, etc)",
  "contributors": [
    "Eric Scouten",
    "Peter Olson"
  ],
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/1999/node-couchdb.git"
  },
  "bugs": {
    "url": "https://github.com/1999/node-couchdb/issues"
  },
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "prepublish": "npm run release:node-0.10 && npm run release:node-0.12 && npm run release:node-4 && npm run release:node-5",
    "release:node-0.10": "./node_modules/.bin/babel lib/node-couchdb.js --no-babelrc --out-file dist/node-0.10.js --presets es2015",
    "release:node-0.12": "./node_modules/.bin/babel lib/node-couchdb.js --no-babelrc --out-file dist/node-0.12.js --plugins array-includes,transform-es2015-spread,transform-es2015-parameters,transform-es2015-destructuring",
    "release:node-4": "./node_modules/.bin/babel lib/node-couchdb.js --no-babelrc --out-file dist/node-4.js --plugins array-includes,transform-es2015-spread,transform-es2015-parameters,transform-es2015-destructuring",
    "release:node-5": "./node_modules/.bin/babel lib/node-couchdb.js --no-babelrc --out-file dist/node-5.js --plugins array-includes,transform-es2015-parameters,transform-es2015-destructuring",
    "test": "./node_modules/.bin/mocha --compilers js:babel-register --timeout 30000 --slow 30000 test/"
  },
  "keywords": [
    "couchdb",
    "memcached",
    "memcache",
    "nosql"
  ],
  "dependencies": {
    "asker": "0.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-es2015-destructuring": "^6.6.5",
    "babel-plugin-transform-es2015-parameters": "^6.7.0",
    "babel-plugin-transform-es2015-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "memcache": "^0.3.0",
    "memcached": "^2.2.1",
    "mocha": "^2.4.5"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  }
}
